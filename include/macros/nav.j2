{%- macro set_classes(name) %}
<ul class="{{ name }}">
  <li class="nav-item">
    <a class="nav-link active" href="#navs">Active</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" href="#navs">Link</a>
  </li>
  <li class="nav-item dropdown">
    <a
      class="nav-link dropdown-toggle"
      href="#navs"
      role="button"
      data-bs-toggle="dropdown"
      aria-expanded="false"
    >Dropdown</a>
    <ul class="dropdown-menu">
      <li><a class="dropdown-item" href="#navs">Action</a></li>
      <li><a class="dropdown-item" href="#navs">Another action</a></li>
      <li><a class="dropdown-item" href="#navs">Something else here</a></li>
      <li class="dropdown-divider"></li>
      <li><a class="dropdown-item" href="#navs">Separated link</a></li>
    </ul>
  </li>
  <li class="nav-item">
    <a class="nav-link disabled">Disabled</a>
  </li>
</ul>
{%- endmacro %}
